<nav>
    <a href="/home">Home</a>
    |
    <% if (userId){%>
        <a href="/profile">Profile</a>
        |
        <a href="/logout">Logout</a>
        |
<% } else { %>
        <a href="/login">Login</a>
        |
        <a href="/signup">Sign Up</a>
        |
 <% } %>
    <a href="/instructors">Instructors</a>
    |
    <a href="/courses">Courses</a>
    |
