<div class="students-container">
    <div class="students-info">
        <div class="students-img">
            <img src="https://icons-for-free.com/iconfiles/png/512/customer+information+personal+profile+user+icon-1320086045331670685.png"
                alt="">
        </div>
        <div class="student-details">
            <p><b>Name:</b>
                <%= instructor.name %>
            </p>
            <p><b>Hidden Talent:</b>
                <%= instructor.talent %>
            </p>
            <p><b>Email:</b>
                <%= instructor.email %>
            </p>
            <p><b>Number:</b>
                <%= instructor.number %>
            </p>
        </div>
    </div>

    <div class="students-courses">
        <!-- <p><b>Enrolled in:</b></p> -->

    </div>

</div>
<div class="main">
    <button class="button">Edit Profile</button>
