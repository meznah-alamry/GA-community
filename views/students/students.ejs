    |
    <a href="/timeline">General Timeline</a>
</nav>

<div class="main">
    <h2 class="title">Student List</h2>

        <% for(key of students){%>

            <div class="card">
                <a href="/students/<%= key.id %>">
                <div class="cardContainer">
                    <h4><b><%= key.name %></b></h4>
                    <p><%= key.course %></p>
                </div>
                </a>
            </div>

        <!-- <a href="/students/<%= key.id %>"><h2 class="desc"><%= key.name %></h2></a>
        <p><%= key.course %></p> -->

        <% } %>



</div>